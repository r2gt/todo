<h2>List</h2>
<ul>
    <% @todo_items.each do |todo_item| %>
        <li>
            <%= todo_item.description %>
            <%= link_to 'Delete', todo_item, method: :delete, id: dom_id(todo_item, :delete) %> |
            <%= link_to 'Edit', edit_todo_item_path(todo_item) %> |
            <%= link_to 'Show', todo_item %>
        </li>
    <% end %>
</ul>
